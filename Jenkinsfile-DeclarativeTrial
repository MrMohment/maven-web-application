@Library('MohmentSharedLibrary') _

pipeline{
  agent any
  tools{
    maven "3.9.2"
  }
  //options{}
  //triggers{}
  stages{
    stage('1GetCode'){
      steps{
        sh "echo Getting the code."
        git 'https://github.com/MrMohment/maven-web-application'
      }
    }
    stage('Build'){
      steps{
        stages("Build")
      }
    }
    stage('SonarQube Report'){
      steps{
        stages("SonarQube Report")
      }
    }
    stage('4Upload'){
      steps{
        sh "echo Uploading"
        sh "mvn deploy"
      }
    }
  }
}
